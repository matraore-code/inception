server {
        listen 443;
        listen [::]:443;
        root /var/www/;
        index index.html index.php index.htm index.nginx-debian.html;
        server_name localhost;
		ssl on;
		ssl_certificate /etc/nginx/ssl/server.crt;
		ssl_certificate_key /etc/nginx/ssl/server.key;
        location / {
                try_files $uri $uri/ =404;
        }
